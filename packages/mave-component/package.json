    "@storybook/addon-links": "^6.4.22",
    "@storybook/testing-library": "^0.0.10",
    "config": "*",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "tsconfig": "*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@fpapado/blurhash": "^1.1.4",
    "lit": "^2.0.2"
